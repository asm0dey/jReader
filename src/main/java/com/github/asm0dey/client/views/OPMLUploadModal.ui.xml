<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
        xmlns:moxie="urn:import:org.moxieapps.gwt.uploader.client">
    <ui:with field="resources" type="com.github.asm0dey.client.resources.Images"/>
    <b:Modal closeVisible="true" animation="true" dynamicSafe="true" keyboard="true" title="OPML Upload">
        <moxie:Uploader ui:field="uploader"/>
        <g:Image resource="{resources.loader}" ui:field="loader" visible="false"/>
        <b:ModalFooter>
            <b:Button type="WARNING" text="Cancel" ui:field="cancelButton"/>
            <!--<b:Button type="PRIMARY" text="Submit" ui:field="submitButton"/>-->
        </b:ModalFooter>
    </b:Modal>
</ui:UiBinder>