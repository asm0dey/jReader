<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
    <g:FocusPanel styleName="feedItem" addStyleNames="unread">
        <g:VerticalPanel>
            <g:HTMLPanel>
                <h2 ui:field="header"></h2>
            </g:HTMLPanel>
            <b:Label ui:field="date"/>
            <b:Paragraph ui:field="content"/>
        </g:VerticalPanel>
    </g:FocusPanel>
</ui:UiBinder>